<h1>Alex's Database</h1>
<div class="wrapper">
  <article class="one_fourth"></article>
  <article class="one_half" id="flash_box">
    <% if flash[:notice] %>
        <p style="color: green"><%= flash[:notice] %>
    <% elsif flash[:registration] %>
        <p style="color: red"><%= flash[:registration] %></p>
    <% end %>
  </article>
  <article class="one_forth"></article>
</div>
<%= yield %>
<h2>END</h2>
